{
    "name":        "arcanedev/geo-location",
    "description": "This package allows you to estimate the real-world geographic location of an object.",
    "homepage":    "https://github.com/ARCANEDEV/GeoLocation",
    "keywords":    [
        "arcanedev", "laravel", "geo", "geocoding", "latitude", "longitude", "distance", "position", "coordinate", "google-maps-api", "directions"
    ],
    "authors":  [
        {
            "name":     "ARCANEDEV",
            "email":    "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/arcanedev-maroc",
            "role":     "Developer"
        }
    ],
    "type":    "library",
    "license": "MIT",
    "require": {
        "php":               ">=7.1.3",
        "arcanedev/support": "~4.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.6.0",
        "phpunit/phpcov":      "~5.0",
        "phpunit/phpunit":     "~7.0",
        "guzzlehttp/guzzle":   "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\GeoLocation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\GeoLocation\\Tests\\": "tests/"
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "(~6.0) Required if you're going to use the Google webservices."
    }
}
